{
  "main": "./AppEntry.js",
  "version": "0.1.1",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "eslint": "eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "tsc": "tsc --noEmit",
    "test": "jest --watch --passWithNoTests",
    "test-ci": "cross-env CI=true jest --env=jsdom --passWithNoTests",
    "prettier": "prettier --ignore-path .eslintignore --parser json --write",
    "test-all": "yarn eslint && yarn test"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn eslint",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{test.ts,test.tsx}": [
      "yarn test-ci",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "resolutions": {
    "@react-native-community/cli-debugger-ui": "4.7.0",
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1",
    "@types/react": "16.9.53"
  },
